{
  "name": "js-flock",
  "version": "3.1.3",
  "description": "Collection of neat modular utilities for bumping up development in NODE and Browser.",
  "main": "index.js",
  "author": "Stefan Novakovic <stefan.novakovich@gmail.com>",
  "contributors": [
    "Daniel Roguljic <droguljic@extensionengine.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/snovakovic/js-flock#readme",
  "engines": {
    "node": ">=6.0.0"
  },
  "bugs": {
    "url": "https://github.com/snovakovic/js-flock/issues"
  },
  "scripts": {
    "build": "node build",
    "publish": "node publish",
    "test:integration": "node test/integration/index.js",
    "test": "istanbul cover --include-all-sources --config ./test/config/istanbul.yml _mocha -- --opts ./test/config/mocha.opts",
    "benchmark:sort": "node benchmark/sort",
    "benchmark:promisify": "node benchmark/promisify",
    "codacy:coverage": "npm test && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snovakovic/js-flock.git"
  },
  "keywords": [
    "deep-freeze",
    "deepFreeze",
    "deepPreventExtensions",
    "deepSeal",
    "enum",
    "es2015",
    "es6",
    "freeze",
    "helpers",
    "last",
    "lastBy",
    "order",
    "order-by",
    "orderBy",
    "poll",
    "preventExtensions",
    "promise",
    "promises",
    "promisify",
    "promisifyAll",
    "seal",
    "sort",
    "sort-array",
    "sort-by",
    "sortBy",
    "then",
    "thenify",
    "toEnum",
    "utility",
    "wait",
    "waitFor"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "codacy-coverage": "^2.0.3",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.2",
    "mocha-lcov-reporter": "^1.3.0",
    "node-fs-extra": "^0.8.2",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^3.3.0",
    "sinon-chai": "^2.13.0"
  },
  "//": "WARNING: Do not remove!.  Private is used to prevent accidental npm publish. Publish should be done only with npm run publish",
  "private": true
